{
  "name": "pr-buddy",
  "productName": "PR Buddy",
  "scripts": {
    "build": "concurrently 'yarn build:main' 'yarn build:renderer'",
    "build:main": "NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./erb/configs/webpack.config.main.prod.js",
    "build:renderer": "NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./erb/configs/webpack.config.renderer.prod.js",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "lint": "NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "package": "rm -rf release/app/dist && yarn build && electron-builder build --publish never",
    "postinstall": "node erb/scripts/check-native-dep.js && electron-builder install-app-deps && NODE_ENV=development webpack --config ./erb/configs/webpack.config.renderer.dev.dll.js && ./patches/apply",
    "dev": "node ./erb/scripts/check-port-in-use.js && yarn dev:renderer",
    "dev:main": "NODE_ENV=development electron -r ts-node/register/transpile-only ./src/main/main.ts",
    "dev:renderer": "NODE_ENV=development webpack serve --config ./erb/configs/webpack.config.renderer.dev.js",
    "dev:script": "ts-node-dev --files --no-notify --respawn --project tsconfig.json",
    "format": "prettier --write ."
  },
  "build": {
    "productName": "PR Buddy",
    "appId": "dev.raine.prbuddy",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": "erb/scripts/notarize.js",
    "mac": {
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "electron-react-boilerplate",
      "repo": "electron-react-boilerplate"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/history": "4.7.9",
    "@types/ini": "^1.3.31",
    "@types/lodash": "^4.14.175",
    "@types/node": "16.10.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.3.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.3.7",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "classnames": "^2.3.1",
    "concurrently": "^6.3.0",
    "core-js": "^3.18.1",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.1.0",
    "detect-port": "^1.3.0",
    "electron": "^15.1.2",
    "electron-builder": "^22.13.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.1.1",
    "electron-rebuild": "^3.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-popper-tooltip": "^4.3.1",
    "react-query": "^3.26.0",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.2",
    "react-router-dom": "^5.3.0",
    "react-test-renderer": "^17.0.2",
    "react-use": "^17.3.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "spinners-react": "^1.0.6",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.2.16",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "ts-pattern": "^3.3.3",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.56.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@octokit/graphql": "^4.8.0",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.1",
    "electron-settings": "^4.0.2",
    "electron-window-state": "^5.0.3",
    "history": "4.x.x",
    "ini": "^2.0.0",
    "lodash": "^4.17.21",
    "p-reduce": "^2.1.0",
    "regenerator-runtime": "^0.13.9",
    "zod": "^3.9.8"
  },
  "browserslist": []
}
